#include <iostream>

int factorial(int n)
{
    if (n <= 1)
        return 1;
    else
        return n * factorial(n - 1);
}
int main() 
{
  try
  {
    int n;
    std::cin >> n;
    if (n < 0)
    {
      throw (0);
    }
    std::cout << factorial(n) << "\n";
  }
  catch (int e)
    {
      std::cout << "Exception occurred. Negative number." << '\n';
    }
  return 0;
}
